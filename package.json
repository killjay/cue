{
  "name": "zoom-vapi-brainstorm-app",
  "version": "1.0.0",
  "description": "Zoom meeting integration app with Vapi.ai voice assistant for AI-powered brainstorming",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": ["zoom", "vapi", "ai", "voice", "brainstorming", "meeting"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@vapi-ai/web": "^2.3.10",
    "@vapi-ai/server-sdk": "^0.10.2",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "socket.io": "^4.7.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  }
}